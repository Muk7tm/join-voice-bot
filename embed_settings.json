    "join_notification": {
      "title": "طلب سحب عضو",
      "description": "الطلب `{request_id}`\n{user_mention} انضم إلى {voice_channel_mention}.\nاضغط **سحب** لنقله إلى قناتك الصوتية.",
      "color": "#2563EB",
      "thumbnail_url": "{user_avatar_url}",
      "fields": [
        {
          "name": "العضو",
          "value": "{user_display_name} (`{user_id}`)",
          "inline": true
        },
        {
          "name": "القناة الصوتية",
          "value": "{voice_channel_name}",
          "inline": true
        }
      ]

{
  "global": {
    "color": "#3B82F6",
    "timestamp": true,
    "thumbnail_url": "{bot_avatar_url}",
    "footer_text": "بوت الانضمام الصوتي",
    "footer_icon_url": "{bot_avatar_url}",
    "log_min_level": "info"
  },
  "embeds": {
    "default": {
      "title": "بوت الانضمام الصوتي",
      "description": "{message}",
      "color": "#3B82F6"
    },
    "log_info": {
      "title": "{level_icon} سجل معلومات",
      "description": "{event}",
      "color": "#0EA5E9",
      "fields": [
        {
          "name": "معرّف الحدث",
          "value": "`{event_id}`",
          "inline": true
        },
        {
          "name": "التفاصيل",
          "value": "{details}",
          "inline": false
        },
        {
          "name": "المنفّذ",
          "value": "{actor_mention}",
          "inline": true
        },
        {
          "name": "الأمر",
          "value": "`{command_name}`",
          "inline": true
        },
        {
          "name": "الطلب",
          "value": "`{request_id}`",
          "inline": true
        },
        {
          "name": "القناة الصوتية",
          "value": "{voice_channel_mention}",
          "inline": true
        },
        {
          "name": "القناة النصية",
          "value": "{text_channel_mention}",
          "inline": true
        },
        {
          "name": "السيرفر",
          "value": "{guild_name} (`{guild_id}`)",
          "inline": true
        }
      ]
    },
